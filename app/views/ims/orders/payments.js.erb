"signType" : "SHA1", //微信签名方式:sha1
"paySign" : "<%= @paySign_val %>" //微信签名
},function(res){
  orderno = "<%= @orderno %>"
  if(res.err_msg == "get_brand_wcpay_request:ok"){
      // get_brand_wcpay_request:ok 发送成功

    window.location.href = "/ims/orders/"+orderno
